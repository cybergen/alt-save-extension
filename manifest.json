{
  "manifest_version": 2,

  "name": "Save to Pics",

  "description": "Save an image to the pics folder",

  "version": "1.0",
  
  "icons": {
    "16" : "icon16.png",
    "48" : "icon48.png",
    "128" : "icon128.png"
  },

  "homepage_url" : "http://www.wizardpolice.com",

  "background" : {
    "scripts" : ["download.js"]
  },

  "permissions": [
    "contextMenus",
    "downloads",
    "tabs"
  ],

  "content_scripts" : [
    {
      "matches" : ["http://*/*", "https://*/*"],
      "js" : ["key_event.js"],
      "run_at" : "document_end"
    }
  ]
}